            "type": "string"
          },
          "GeoLoc": {
            "type": "string",
            "maxLength": 18,
            "minLength": 18,
            "description": "format (rrccc-xxxxxxxxxxxx)\nrr: Region, ccc: City (GeoLocation document)\nx..x: Divisions within the requesting party\n\n\nThe Request initiator geo location and the internal division inside the entity which will be agreed upon on a requesting parties one-on-one basis"
          },
          "GeoLocDsc": {
            "maxLength": 200,
            "minLength": 1,
            "pattern": "[^!@#$%?*/^\\\\(){}+=_|\\[\\];<>\"'&]+",
            "type": "string"
          }
        },
